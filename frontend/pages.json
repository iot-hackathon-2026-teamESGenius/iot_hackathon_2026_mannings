{
  "easycom": {
    "autoscan": true,
    "^uni-(.*)": "@/uni_modules/uni-$1/components/uni-$1/uni-$1.vue",
	"^qiun-(.*)": "@/uni_modules/qiun-$1/components/qiun-$1/qiun-$1.vue"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/my", 
      "style": {
        "navigationBarTitleText": "个人中心",
		"navigationStyle": "custom"
      }
    },
    {
      "path": "pages/index/replenishment",
      "style": { 
		  "navigationBarTitleText": "补货管理" ,
		  "navigationStyle": "custom"
		  }
	},
	{
		"path": "pages/login",
		"style": {
			"navigationBarTitleText": "登录"
		}
	}
  ,{
	"path": "pages/index/deliever_map",
	"style": {
		"navigationBarTitleText": "配送地图",
		"navigationStyle": "custom"
	}
},
{
	"path": "pages/index/forcast",
	"style": {
		"navigationBarTitleText": "需求预测",
		"navigationStyle": "custom"
	}
}
],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
