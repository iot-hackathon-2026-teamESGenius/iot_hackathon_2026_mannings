      },
      {
        "pagePath": "pages/index/replenishment",
        "text": "补货",
        "iconPath": "static/img/replenishment.png",
        "selectedIconPath": "static/img/replenishment_selected.png"
      },
      {
        "pagePath": "pages/index/deliever_map",
        "text": "调度",
        "iconPath": "static/img/deliever.png",
        "selectedIconPath": "static/img/deliever_selected.png"
      },
      {
        "pagePath": "pages/my",
